{
  "compilerOptions": {
    "strictNullChecks": true,
    "experimentalDecorators": true,
    "jsx": "preserve",
    "noUnusedParameters": true,
    "noUnusedLocals": true,
    "noImplicitAny": true,
    "target": "es5",
    "module": "es6",
    
  },
  "include": [
    "src/**/*",
    "ts",
    "tsx"
  ],
  "exclude": [
    "node_modules",
    "lib",
    "scripts"
  ]
}